{
    "name": "hnassr/nova-key-value",
    "description": "A Laravel Nova field.",
    "version": "master",
    "source": {
        "url": "https://github.com/64robots/nova-key-value",
        "type": "git",
        "reference": "master"
    },
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Hnassr\\NovaKeyValue\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hnassr\\NovaKeyValue\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
